{
  "manifest_version": 2,

  "name": "Embed buttons",
  "description": "Chrome Extension - Add Embed buttons links to popular webstes",
  "version": "1.0",

  "permissions": [
    "tabs",
    "*://www.twitch.com/*",
    "*://go.twitch.com/*"
  ],
  "content_scripts": [
    {
      "matches": [
        "*://www.twitch.tv/*",
        "*://go.twitch.tv/*"
      ],
      "js": ["embedder.js"],
      "run_at": "document_end"
    }
  ]
}